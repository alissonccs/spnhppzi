fu.max=fu.max,
cens.prob = 0,
dist.x = c("binomial","normal"),
par.x=list(0.7, c(0, 1)),
beta.x=c(beta1_r, beta2_r)
)
set.seed(NULL)
# GERA DADOS ====
run_montecarlo <- function(step){
set.seed(258258*step)
base_sp<-spsimrec8.1(N=cov.fu$N,
cov_rec=c("ID","X1","X2"),
# cov_log=c("X3","X4"),
beta_x_rec = c(beta1_r,beta2_r),
# beta_x_log = c(psi1_r,psi2_r),
logist = 0,
x1 = cov.fu$x1,
fu = cov.fu$fu,
fu_max = cov.fu$fu.max,
fu_min = cov.fu$fu.min,
spatial=1,
list_area=list_area_RMBH,
sp_model="ICAR",
SP_N=100,
nb_mat=RMBH_mat,
sp_tau=sp_tau_r,
#sp_alpha=sp_alpha_r,
#beta.x = cov.fu$beta.x,
# dist_z = "lognormal",
random_ef=1,
tp_rnd_ef=0,
pi = pi_r,
par_z=0,
dist_int_func = "weibull",
par_int_func = c(alpha1_r,alpha2_r)
)
base_sp<-base_sp %>%
filter(recurr==0) %>%
group_by(ID) %>%
summarise(ngroup1=max(ngroup1),rnd_ef=max(rnd_ef),X1=max(X1),X2=max(X2))
media_sp<-base_sp  %>%
summarise(
media = mean(ngroup1),
mediana = median(ngroup1)
)
write.table(media_sp, file="/home/alisson/UFMG/TESE/SIM/RESULT_01_08/NOTE/media_sp.txt",
row.names = FALSE,
col.names = FALSE,
append = TRUE)
return(media_sp)
}
